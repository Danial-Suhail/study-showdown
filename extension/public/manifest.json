{
  "manifest_version": 3,
  "name": "Study Showdown",
  "description": "An engaging way to study!",
  "version": "1.0.0",
  "action": {
    "default_popup": "./index.html",
    "default_icon": "./openai-avatar.png"
  },
  "permissions": ["tabs", "activeTab", "storage"],
  "background": { 
    "service_worker": "background.js"
  }
}
